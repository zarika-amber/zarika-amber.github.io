# Ignore node_modules folder
node_modules/

# Ignore build files (like dist, bundle.js)
dist/

# Ignore log files
*.log

# Ignore temporary files from editors or OS
*.swp
*.swo
.DS_Store
Thumbs.db

# Ignore npm debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore IDE files (for example VS Code or JetBrains)
.vscode/
.idea/
*.sublime-workspace
*.sublime-project








